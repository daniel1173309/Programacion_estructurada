// Parte_1
#include <stdio.h>

int main() {
    int num;
    printf("Ingrese un numero entero positivo: ");
    scanf("%d", &num);
    if(num<0){
        printf("Error");
    }

    printf("Los factores de %d son: ", num);
    for (int i = 1; i <= num; i++) {
        if (num % i == 0) {
            printf("%d ", i);
        }
            
    }
    
    return 0;
}

//Parte_2
